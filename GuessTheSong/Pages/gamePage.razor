@inject NavigationManager Navigation

<div id="header">
    <div class="label-left">
        <h3>Guess the song</h3>
    </div>
    <div class="center-container">
        <div class="score-container">
            <h2 class="score-label">Your score</h2>
            <hr id = "score divider">
            <div class="score-display">21</div>
        </div>
    </div>
    <div class="label-right">
        <h3 id="name-label"><p>Username: </p>name</h3>
    </div>
</div>


<div id="song-options">
    <h3 id="song-options-label">Pick your option</h3>
    <div id="grey-box">
        <div class="song-option-element"></div>
        <div class="song-option-element"></div>
        <div class="song-option-element"></div>
    </div>
</div>