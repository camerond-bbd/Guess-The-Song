@page "/Profile/{ID}"
<PageTitle>Profile</PageTitle>
<div class="page">
    <h1 class="title">Hello, @loggedinUser.username</h1>

    <div class="progressContainer">
        <div class="progressBar"></div>
    </div>
    <p>Your total Score</p>

    <p>You are scoring more than of users</p>
    <section class="button-section">
        <button @onclick="goToGameClick">Go to game:</button>
    </section>

    <GuessTheSong.components.PopupMessage></GuessTheSong.components.PopupMessage>
</div>

@code {
    [Parameter]
    public int ID { get; set; }
}